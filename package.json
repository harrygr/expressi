{
  "name": "expressi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev:server": "DEBUG=expressi:server nodemon --watch 'src/**/*.ts' --watch 'views/**/*.hbs' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/server.ts",
    "dev:web": "node ./react-ui/scripts/watch.js",
    "dev": "yarn dev:server && yarn dev:web",
    "build": "tsc",
    "serve": "node ./dist/server.js"
  },
  "dependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/cookie-parser": "^1.3.30",
    "@types/debug": "^0.0.29",
    "@types/express": "^4.0.36",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8.0.14",
    "@types/serve-favicon": "^2.2.28",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "hbs": "~4.0.1",
    "morgan": "~1.8.1",
    "nodemon": "^1.11.0",
    "serve-favicon": "~2.4.2",
    "ts-node": "^3.2.0",
    "typescript": "^2.4.1"
  }
}